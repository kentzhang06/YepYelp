<!DOCTYPE html>
<html>
  <head>
    <title>YepYelp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Headland+One&display=swap" rel="stylesheet">
  </head>

  <body>
    <script type="text/javascript">
      window.fishUrl = "<%= image_url('fish.png') %>";
    </script>
    <script type="text/javascript">
      window.narutoRamenUrl = "<%= image_url('naruto-ramen.png') %>";
    </script>
    <script type="text/javascript">
      window.signupNarutoUrl = "<%= image_url('signup_login_pic.png') %>";
    </script>
    <%= yield %>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
  </body>
</html>
